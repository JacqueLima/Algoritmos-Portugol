programa
{
	//
Exerc.2 Criar um algoritmo para escrever 10 vezes seu nome
	cadeia nome
	inteiro n
	inteiro name
	
	funcao inicio()
	{
		escreva("Digite seu nome: ")
		leia(nome)
		lerNome()
		
		
		
	}
	funcao lerNome()
	{
		
		para(n=0;n<=10;n++){
		escreva("\n",n,".",nome)
		
		}
	}

}
